{
  "name": "movie-library",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">=7.10"
  },
  "scripts": {
    "start": "node src/index.js",
    "test": "NODE_ENV=test mocha --recursive \"src/**/*.spec.js\"",
    "test:acceptance": "NODE_ENV=test mocha --recursive \"src/**/*.accept.js\"",
    "standard": "standard",
    "pretest": "standard",
    "nodemon": "NODE_ENV=development nodemon --watch src -L src/index.js",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "body-parser": "^1.17.1",
    "cors": "^2.8.3",
    "express": "^4.15.2",
    "mongodb": "^2.2.27",
    "morgan": "^1.8.1",
    "snyk": "^1.30.1",
    "verror": "^1.10.0"
  },
  "snyk": true,
  "devDependencies": {
    "chai": "^4.0.0",
    "dotenv": "^4.0.0",
    "gulp": "^3.9.1",
    "mocha": "^3.4.2",
    "run-sequence": "^1.2.2",
    "sinon": "^2.3.2",
    "standard": "^10.0.2",
    "supertest": "^3.0.0"
  },
  "standard": {
    "ignore": [
      "docs/"
    ]
  }
}
